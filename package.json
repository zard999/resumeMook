{
  "name": "resume",
  "version": "0.0.1",
  "author": "zyh",
  "description": "resume",
  "main": "./dist/electron.js",
  "scripts": {
    "start:dev": "npm run start:render && npm run start:main",
    "start:main": "webpack --config ./webpack/webpack.main.dev.js && electron ./dist/electron.js",
    "start:render": "webpack-dev-server --config ./webpack/webpack.render.dev.js",
    "install:electron": "ELECTRON_MIRROR=https://cdn.npm.taobao.org/dist/electron/ npm install electron",
    "build:main": "webpack --config ./webpack/webpack.main.prod.js",
    "build:render": "webpack --config ./webpack/webpack.render.prod.js",
    "pack": "npm run build:main && npm run build:render && electron-builder --dir",
    "dist": "npm run build:main && npm run build:render && electron-builder",
    "win": "npm run build:main && npm run build:render && electron-builder --win --ia32"
  },
  "build": {
    "appId": "com.resume.platform",
    "productName": "简历平台",
    "copyright": "Copyright © 2022 ${author}",
    "extends": null,
    "files": [
      "dist/**/*",
      "package.json",
      "node_modules/"
    ],
    "directories": {
      "output": "package"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "nsis": {
      "language": 2052,
      "artifactName": "react_view.exe",
      "allowElevation": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    }
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@reduxjs/toolkit": "^1.8.5",
    "classnames": "^2.3.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^8.0.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "redux": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.14.3",
    "@babel/core": "7.14.3",
    "@babel/plugin-transform-modules-commonjs": "7.14.0",
    "@babel/plugin-transform-runtime": "7.14.3",
    "@babel/preset-env": "7.14.2",
    "@babel/preset-react": "7.13.13",
    "@babel/preset-typescript": "7.13.0",
    "@types/lodash": "^4.14.184",
    "@types/react": "^17.0.5",
    "@types/react-css-modules": "4.6.2",
    "@types/react-dom": "^17.0.5",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "autoprefixer": "9.0.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-react-css-modules": "5.2.6",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "6",
    "css-loader": "3.0.0",
    "electron": "^11.1.1",
    "electron-builder": "^23.3.3",
    "eslint": "^7.26.0",
    "eslint-config-alloy": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "4.3.0",
    "less": "3.12.2",
    "less-loader": "6.2.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^1.0.0",
    "optimize-css-assets-webpack-plugin": "4.0.0",
    "path-to-regexp": "6.2.0",
    "postcss-less": "3.1.4",
    "postcss-loader": "3.0.0",
    "prettier": "^2.3.0",
    "react-router-cache-route": "^1.12.11",
    "style-loader": "2.0.0",
    "terser-webpack-plugin": "4.2.3",
    "typescript": "4.2.4",
    "url-loader": "^4.1.1",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "^5.8.0"
  }
}
